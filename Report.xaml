<Window x:Class="ComShop.Report"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ComShop"
        WindowState="Maximized"
        ResizeMode="NoResize"
        mc:Ignorable="d"
        Title="Отчёты" Height="720" Width="1280">
    <Grid>
        <Label x:Name="cld_start" Content="Дата начала" HorizontalAlignment="Left" Margin="8,10,0,0" VerticalAlignment="Top" Width="179" FontSize="14" HorizontalContentAlignment="Center"/>
        <Label x:Name="cld_end" Content="Дата окончания" HorizontalAlignment="Left" Margin="238,10,0,0" VerticalAlignment="Top" Width="179" FontSize="14" HorizontalContentAlignment="Center"/>
        <Calendar HorizontalAlignment="Left" Margin="8,39,0,0" VerticalAlignment="Top"/>
        <Calendar HorizontalAlignment="Left" Margin="238,39,0,0" VerticalAlignment="Top"/>
        <Button Content="Рассчитать" HorizontalAlignment="Left" Margin="8,212,0,0" VerticalAlignment="Top" Width="407" Height="41" FontSize="30" Click="RunReport"/>


    </Grid>
</Window>
